<!-- <h1>Photos</h1> -->
<%#= link_to 'New Photo', new_photo_path %>
<%#= link_to 'contacts', contacts_index_path, :id=>"clr"%>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <!--<th>Content</th>-->
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%=image_tag photo.image ,:class=>"img"%></td>
        <td><%= photo.title %></td>
        <!--<td><%= photo.content.html_safe %></td>-->
        <td><%= photo.user.name %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'show_comments', users_commants_path(photo) %> </td>
        <td><%= link_to '<i class="fa fa-pencil-square-o" aria-hidden="true"></i>
'.html_safe, edit_photo_path(photo) %> </td>
        <td><%= link_to '<i class="fa fa-trash" aria-hidden="true"></i>
'.html_safe, photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>
<style> 
  .img{
    height:500px;
    width:500px;
  }
  .image{
    float: left
    color: green;
  }
